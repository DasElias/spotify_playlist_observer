{% if songImage is null %}
	<div class="bg-darkgrey-400 flex items-center justify-center w-full h-full text-darkgrey-100 text-xl rounded-sm">
		<i class="bi bi-music-note-beamed"></i>
	</div>
{% else %}
	<img aria-hidden="false" draggable="false" loading="eager" src="{{ songImage }}" class="w-full h-full">
{% endif %}
